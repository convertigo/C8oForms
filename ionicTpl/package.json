    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "ionic:build:fast": "npm run wv && node --max_old_space_size=8048 node_modules/@angular/cli/bin/ng build app --configuration=fastbuild",
        "ionic:build:watch": "npm run wv && node --max_old_space_size=8048 node_modules/@angular/cli/bin/ng build app --configuration=fastbuild --watch=true",
        "ionic:build:prod": "npm run wv && node --max_old_space_size=8048 node_modules/@angular/cli/bin/ng build app --configuration=production",
        "ionic:serve": "npm run wv && node --max_old_space_size=8048 node_modules/@angular/cli/bin/ng serve app --hmr=true",
        "wv": "node .\/version.js"
    },
    "private": true,
